<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.imbuegen.alumniapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CompanyActivity" />
        <activity android:name=".AlumniActivity" />
        <activity android:name=".AlumniInfoActivity" />
        <activity android:name=".DepartmentsActivity" />
        <activity android:name=".RegistrationActivity" android:theme="@style/mNoActionBar"/>
        <activity android:name=".LoginActivity" android:theme="@style/mNoActionBar"/>
    </application>

</manifest>